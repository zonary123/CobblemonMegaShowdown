{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:tera_bug",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/generic/sparkle/glowingsparkle.png"
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 20,
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 1
			},
			"minecraft:emitter_shape_disc": {
				"radius": "0.7 * (q.entity_size / 2)",
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "1 * math.max(1, q.entity_size / 2)"
			},
			"minecraft:particle_initial_speed": "1 * (q.entity_size / 2)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, "5 * (q.entity_size / 2)", 0],
				"linear_drag_coefficient": "4 * (q.entity_size / 2)"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.12 * math.min(1, (q.entity_size / 2 <= 0.4 ? 0.4 : q.entity_size / 2))", "0.12 * math.min(1, (q.entity_size / 2 <= 0.4 ? 0.4 : q.entity_size / 2))"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 8,
					"texture_height": 32,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [0, 8],
						"frames_per_second": 4,
						"max_frame": 4,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime * v.particle_random_4",
					"gradient": {
						"0.0": "#FF9AF257",
						"0.64": "#FFC4FBF8",
						"0.86": "#FFFFFFFF"
					}
				}
			}
		}
	}
}