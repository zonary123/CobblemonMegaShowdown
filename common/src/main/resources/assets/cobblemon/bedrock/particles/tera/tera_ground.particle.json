					"texture_width": 45,
					"texture_height": 9,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [9, 9],
						"step_UV": [9, 0],
						"frames_per_second": 10,
						"max_frame": 5,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime * v.particle_random_4",
